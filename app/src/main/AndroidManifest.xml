<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.honeybee">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Honeybee"
        android:usesCleartextTraffic="true">
        <activity android:name=".login.loginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".calendar.calendarMainActivity" />
        <activity android:name=".calendar.calendarPlusActivity"/>
        <activity android:name=".chat.chatMainActivity"/>
        <activity android:name=".mainActivity"/>
        <activity android:name=".story.storyMainActivity"/>
        <activity android:name=".register.registerActivity"/>
        <activity android:name=".locationMainActivity"/>
        <activity android:name=".story.storyPostActivity"/>
        <activity android:name=".quest.questsActivity"/>
        <activity android:name=".myinfo.myinfoActivity"/>
        <activity android:name=".myinfo.statemessageActivity"/>
        <activity android:name=".money.moneysendActivity"/>
        <activity android:name=".money.moneychargeActivity"/>
        <activity android:name=".money.moneyActivity"/>
        <activity android:name=".quest.questActivity">


        </activity>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />
        <uses-library android:name=" org.apache.http.legacy" android:required="false"/>
    </application>

</manifest>